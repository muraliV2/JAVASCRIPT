//reduce is nothing but it provides the op in the singel value


let num = [1,2,3,4,5]
let hi = num.reduce(max)
console.log(hi)

let hi1 = num.reduce(min)
console.log(hi)

function sum(acc,element)
{
    return acc + element
}

function max(acc,element)
{
    return Math.max(acc,element)
}

function min(acc,element)
{
    return Math.min(acc,element)
}
